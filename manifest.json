{
    "manifest_version": 3,
    "name": "lc-blurring",
    "version": "1.0.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://leetcode.com/problems/*"
            ],
            "exclude_matches": [
                "https://leetcode.com/problems/*/submissions/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}